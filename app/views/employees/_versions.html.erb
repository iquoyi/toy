<div class="card shadow-sm my-4">
  <div class="card-header">
    <h4 class="mb-0">List of versions</h4>
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-striped table-hover table-bordered text-center">
        <thead>
          <tr>
            <th>ID</th>
            <th>Master Id</th>
            <th>First name</th>
            <th>Last name</th>
            <th>Birthday</th>
            <th>Address</th>
            <th>Valid From</th>
            <th>Valid To</th>
            <th>Created At</th>
            <th>Expired At</th>
          </tr>
        </thead>

        <tbody>
          <% versions.each do |version| %>
            <tr>
              <td><%= version.id %></td>
              <td><%= version.master_id %></td>
              <td><%= version.first_name %></td>
              <td><%= version.last_name %></td>
              <td><%= version.birthday %></td>
              <td><%= version.address %></td>
              <td><%= version.valid_from %></td>
              <td><%= version.valid_to %></td>
              <td><%= version.created_at %></td>
              <td><%= version.expired_at %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

